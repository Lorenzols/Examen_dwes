
<div class="c_lista">
    <h1 class="t_c">Listar</h1>
    <div class="c_list">  
    {{#each coches}}
        <div class="c_coche_list">
            <h1>{{marca}}</h1>
            <h1>{{potencia}}</h1>
            <div class="c_img">
                <img src="{{url}}" alt="">
            </div>
            <div class="c_boton">
                <button class="boton" type="submit"><a href="/coches/edit/{{_id}}">Edit</a></button>
                <button class="boton" type="submit"><a href="/coches/delete/{{_id}}">Delete</a></button>
            </div>
        </div>
    {{else}}
        <h1>No hay nada</h1>
    {{/each}}
</div>
</div>